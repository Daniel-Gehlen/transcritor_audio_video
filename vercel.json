{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "bash build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "functions": {
    "app.py": {
      "memory": 3008,
      "maxDuration": 30,
      "payloadSizeLimit": "50mb"
    }
  }
}